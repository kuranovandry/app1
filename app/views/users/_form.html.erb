
<h1>Editing user</h1>

<%= form_for @user do |f| %>

<p>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_namer %>
</p>

<p>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name %>
</p>


<p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>


<p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
</p>
	
<p>
  <%= f.submit %>
</p>


<% end %>
